	<script type="text/javascript">

        pageTitle = function(){
            const page = window.location.hash;
            let title = document.getElementById("menu");

            switch(page){
                case "#page1":
                    title.innerText = "Smart Sensor";
                    break;
                case "#page2":
                    title.innerText = "sensor monitoring";
                    break;
                case "#page3":
                    title.innerText = "log";
                    break;
                case "#page4":
                    title.innerText = "개발자";
                    break;
            }
        }

        // #ccddff    #b8cfe0
        // #105d94', '#7BAABE', 'whitesmoke'
		$(document).ready(function() {
			$('#fullpage').fullpage({
                anchors: ['page1', 'page2', 'page3', 'page4'],
				sectionsColor: ['#105d94', '#105d94', '#ccddff', '#000'],
                slidesNavigation: true
			});

            
            pageTitle();

            PH_pageMove();
            RTD_pageMove();
            SALT_pageMove();
            DO_pageMove();
            
		});

        $(window).bind('wheel', pageTitle);
	</script>

